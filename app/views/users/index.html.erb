<h1>User list</h1>
<%= button_to 'sign out', [:destroy_user_session], method: :delete %>
<ul>
  <% @users.each do |user|%>
    <li>
      <img src="<%= user.photo %>" alt="user photo">
      <h2><%= link_to user.name, user %></h2>
      <span>Number of posts: <%= user.posts_counter %></span>
    </li>
  <% end %>
</ul>
